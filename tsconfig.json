{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "ES2020"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@autojs"
    ],
    "allowJs": true,
    "sourceMap": false,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  },
  "include": [
    "src/**/*",
    "@auto.pro/core/src/global.d.ts"
  ],
  "exclude": [
    "node_modules",
    "out"
  ]
}